{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "testapp": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/testapp",
      "sourceRoot": "projects/testapp/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/testapp",
            "index": "projects/testapp/src/index.html",
            "main": "projects/testapp/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/testapp/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/testapp/src/favicon.ico",
              "projects/testapp/src/assets",
              "projects/testapp/src/manifest.webmanifest"
            ],
            "styles": [
              "projects/testapp/src/styles.scss"
            ],
            "scripts": [],
            "serviceWorker": true,
            "ngswConfigPath": "projects/testapp/ngsw-config.json"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",

          "configurations": {
            "production": {
              "browserTarget": "testapp:build:production",
              "verbose": true
            },
            "development": {
              "browserTarget": "testapp:build:development",
              "verbose": true
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "testapp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/testapp/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/testapp/src/favicon.ico",
              "projects/testapp/src/assets",
              "projects/testapp/src/manifest.webmanifest"
            ],
            "styles": [
              "projects/testapp/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "ngsllayouts": {
      "projectType": "library",
      "root": "projects/ng-sl-layouts",
      "sourceRoot": "projects/ng-sl-layouts/src",
      "prefix": "sl",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng-sl-layouts/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-sl-layouts/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-sl-layouts/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ng-sl-layouts/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "ngslcommoncontrols": {
      "projectType": "library",
      "root": "projects/ng-sl-common-controls",
      "sourceRoot": "projects/ng-sl-common-controls/src",
      "prefix": "sl",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng-sl-common-controls/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-sl-common-controls/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-sl-common-controls/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ng-sl-common-controls/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
